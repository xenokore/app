{
    "name": "xenokore/app",
    "description": "The core app of the Xeno framework",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Yani",
            "email": "yani@xenokore.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xenokore\\App\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Xenokore\\App\\Tests\\": "tests/",
            "Xenokore\\App\\Tests\\Data\\Test\\": "tests/data/src/"
        }
    },
    "require": {
        "psr/log": "^1.1",
        "php-di/php-di": "^6.3",
        "symfony/dotenv": "^5.2",
        "xenokore/utility": "^1.0",
        "xenokore/component-loader": "^1.0",
        "doctrine/annotations": "^1.12",
        "php-di/slim-bridge": "^3.1",
        "slim/slim": "^4.7",
        "slim/psr7": "^1.4",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "escapestudios/symfony2-coding-standard": "^3.12"
    }
}
